# ============================================
# Supabase 설정 (필수)
# ============================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# ============================================
# 카카오 설정 (필수)
# ============================================
# 카카오 개발자 > 내 애플리케이션 > 앱 키
KAKAO_REST_API_KEY=your-rest-api-key
KAKAO_REDIRECT_URI=http://localhost:8000/auth/kakao/callback

# 카카오 로그인 후 발급되는 토큰 (동적으로 관리)
KAKAO_ACCESS_TOKEN=your-access-token
KAKAO_REFRESH_TOKEN=your-refresh-token

# 카카오톡 채널 (비즈니스용)
KAKAO_CHANNEL_ID=your-channel-id
KAKAO_SENDER_KEY=your-sender-key

# ============================================
# 알림톡 (NHN Cloud) - 선택, 유료
# ============================================
NHN_ALIMTALK_APP_KEY=your-app-key
NHN_ALIMTALK_SECRET_KEY=your-secret-key

# ============================================
# Notion 설정 (선택)
# ============================================
NOTION_TOKEN=your-notion-integration-token
NOTION_DATABASE_ID=your-database-id
